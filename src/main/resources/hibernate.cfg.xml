<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="">
  <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
  <property name="hibernate.connection.driver_class">com.ibm.db2.jcc.DB2Driver</property>
  <property name="hibernate.connection.password">vtauser</property>
  <property name="hibernate.connection.url">jdbc:db2://localhost:50000/VTA_DB:retrieveMessagesFromServerOnGetMessage=true;</property>
  <property name="hibernate.connection.username">VTAUSER</property>
  <property name="hibernate.default_schema">VTA_DB_SCHEMA</property>
  <property name="hibernate.dialect">org.hibernate.dialect.DB2Dialect</property>
  <property name="hibernate.search.autoregister_listeners">false</property>
  <property name="hibernate.show_sql">true</property>
  <property name="hibernate.format_sql">true</property>
  <mapping class="main.java.com.boa.domain.ProgressIndicatorsBO"
   package="main.java.com.boa.domain" resource="main/resources/ProgressIndicators.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.PrioritiesBO"
   package="main.java.com.boa.domain" resource="main/resources/Priorities.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.RoleTransactionLimitBO"
   package="main.java.com.boa.domain" resource="main/resources/RoleTransactionLimit.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.TransactionsBO"
   package="main.java.com.boa.domain" resource="main/resources/Transactions.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.CodesBO"
   package="main.java.com.boa.domain" resource="main/resources/Codes.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.LimitsBO"
   package="main.java.com.boa.domain" resource="main/resources/Limits.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.CategoriesBO"
   package="main.java.com.boa.domain" resource="main/resources/Categories.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.RolesBO"
   package="main.java.com.boa.domain" resource="main/resources/Roles.hbm.xml"/>
  <mapping class="main.java.com.boa.domain.AgentsBO"
   package="main.java.com.boa.domain" resource="main/resources/Agents.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
